{
  "name": "homepage_front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.19.3",
    "axios": "^0.24.0",
    "craco-less": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint:staged": "lint-staged",
    "lint": "eslint --ext .tsx --ext .ts ./src",
    "lint:fix": "eslint ./src/**/*.{tsx,ts} --fix",
    "format": "prettier --check ./src/**/*.{tsx,ts}",
    "write": "prettier --write ./src/**/*.{tsx,ts}",
    "stylelint": "stylelint '**/*.scss' --custom-syntax postcss-scss",
    "stylelint:fix": "stylelint '**/*.scss' --custom-syntax postcss-scss --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "debug": "^4.3.3",
    "env-cmd": "^10.1.0",
    "eslint": "8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.3",
    "next-sitemap": "^2.5.7",
    "postcss": "^8.4.6",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "redux-logger": "^3.0.6",
    "stylelint": "^14.3.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "yarn write",
      "git add ."
    ],
    "**/*.less": [
      "yarn stylelint:fix",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  }
}
